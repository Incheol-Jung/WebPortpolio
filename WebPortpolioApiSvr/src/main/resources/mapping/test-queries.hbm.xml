<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<sql-query name="test.getTestListALL">	
	<return alias="t" class="org.jij.WebPortpolio.model.Test" />
	<![CDATA[	
		SELECT t.*		
		FROM TEST as t 
		ORDER BY t.test_id DESC		
	]]>
	</sql-query>
	
	<sql-query name="test.getTestListDetail">	
	<return alias="t" class="org.jij.WebPortpolio.model.Test" />
	<![CDATA[	
		SELECT t.*		
		FROM TEST as t 
		WHERE t.test_id = :testId
	]]>
	</sql-query>	

	
</hibernate-mapping>